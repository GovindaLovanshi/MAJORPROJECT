<% layout("/layouts/boilerplate") -%>

    <div class="row mt-3">
        <h1 class="col-6 offset-3">Log In On WanderLust</h1>
        <div class="col-6 offset-3">
            <form class="needs-validation " novalidate method="POST" action="/login">
                <div class="mb-3">
                    <lABEl for="username" class="form-label">SignUp</lABEl>
                    <input name="username" id="username" type="text" class="form-control" required/>

                </div>


                <div class="mb-3">
                    <lABEl for="password" class="form-label">Password</lABEl>
                    <input name="password" id="username" type="password" class="form-control" required/>

                </div>
                <button class="btn btn-success">Log In</button>

            </form>
        </div>
    </div>